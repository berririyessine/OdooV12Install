<odoo>
    <data>
        <!--Appel D'offre tuneps-->
              <record id="appel_offre_tuneps_form_view" model="ir.ui.view">
        <field name="name">offre.tuneps.form</field>
        <field name="model">offre.tuneps</field>
        <field name="arch" type="xml">
            <form>
                <header>
                   <button string="QCM d'Evaluation"
                           context="{'default_qcm_id': id,'default_prospection_id': prospection_id}"
                           name="QCM_tuneps" type="object" class="btn-primary"/>
                </header>
                <sheet>
                    <div class="oe_title">
                        <label for="name"  string="Référence"/>
                        <h1><field name="name" /></h1>
                    </div>
                    <group>
                        <group>
                            <field name="prospection_id" string="Prospection"/>
                            <field name="partner_id" string="Client"/>
                        </group>
                        <group>
                            <field name="date_start" string="Date Début"/>
                            <field name="date_end" string="Date Fin"/>
                        </group>
                        <group>
                            <field name="caution" string="Caution"/>
<!--                            <field name="piece_joint" string="Cahier Des Charges" filename="file_name"/>-->
                        <field name="piece_joint" widget="many2many_binary"/>
                        </group>

                    </group>

                </sheet>
            </form>
        </field>
        </record>
       <record id="workflow_tender.create_offre_tuneps" model="ir.actions.act_window">
            <field name="name">Appel d'offre TUNEPS</field>
            <field name="res_model">offre.tuneps</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="appel_offre_tuneps_form_view"/>
        </record>
             <!--      QCM OFFRE TUNEPS-->
        <record id="qcm_offre_tuneps_form_view" model="ir.ui.view">
        <field name="name">qcm.offre.tuneps.form</field>
        <field name="model">qcm.offre.tuneps</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                             <field name="qcm_id" string="Offre Tuneps"/>
                             <field name="prospection_id" string="Prospection"/>
                        </group>
                    </group>
                    <group>
                        <group>
                            <field name="q1" string="1-la tache est désigner au service spécialisée"/>
                            <field name="q2" string="2-consultation de contenu reçu"/>
                        </group>
                        <group>
                            <field name="q3" string="3-ajustemet et update de contenu et tache demander"/>
                            <field name="q4" string="4-Tache Fini avec Succès"/>
                        </group>
                        <group>
                            <field name="comment" string="Ajouter Commentaire"/>
                        </group>

                    </group>
                    <footer>
                        <button string="Valider" name="Valider" type="object" class="btn-primary"/>
                        <button string="Annuler" class="btn-default" special="cancel"/>
                    </footer>

                </sheet>


            </form>
        </field>
        </record>
        <record id="action_qcm_tuneps" model="ir.actions.act_window">
            <field name="name">QCM TUNEPS</field>
            <field name="res_model">qcm.offre.tuneps</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="qcm_offre_tuneps_form_view"/>

            <field name="target">new</field>
        </record>

    </data>
</odoo>